.customNavBarWrap {
    background-color: #2f2f2f;
    position: fixed;
}

.cm-con,.customNavBarWrap {
    left: 0;
    top: 0;
    width: 100vw;
    z-index: 9999;
}

.cm-con {
    height: 43px;
}

.customNavBarBox {
    height: 32px;
    padding-left: 7px;
    position: relative;
}

.customNavBarBox,.leftBtnArea {
    align-items: center;
    display: flex;
}

.leftBtnArea {
    background: hsla(0,0%,100%,.6);
    border: 1px solid transparent;
    border-radius: 60px;
    box-sizing: border-box;
    height: 64px;
    margin-right: 5px;
    transition: top .2s;
    width: 176px;
}

.leftBtnArea.black {
    border-color: rgba(0,0,0,.08);
}

.leftBtnArea.white {
    border-color: hsla(0,0%,100%,.2);
}

.leftBtnArea.needBlackBg {
    background: rgba(0,0,0,.15);
}

.leftBtnArea.needBlackBg.android {
    background: rgba(0,0,0,.18);
}

.leftBtnArea>.view {
    box-sizing: border-box;
    height: 32px;
    position: relative;
    width: 86px;
}

.leftBtnArea>.view:last-child {
    border-left: 1px solid transparent;
}

.leftBtnArea.singleBack,.leftBtnArea.singleHome {
    width: 32px;
}

.leftBtnArea.black>.view:first-child {
    border-right: 1px solid rgba(0,0,0,.08);
}

.leftBtnArea.white>.view:first-child {
    border-right: 1px solid hsla(0,0%,100%,.2);
}

.leftBtnArea.singleBack>.view:first-child,.leftBtnArea.singleHome>.view:first-child {
    border-right: none;
}

.leftBtnArea .image {
    height: 26.5px;
    left: 50%;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 26.5px;
}

.leftBtnArea .iconImg {
    background-size: 100%auto;
    height: 20px;
    width: 20px;
}

.title-box {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    left: 50%;
    max-width: 382rpx;
    padding: 0 30rpx;
    position: absolute;
    text-align: center;
    transform: translateX(-50%);
    z-index: 999;
}

.title-box view {
    font-weight: 700;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.title-arr-box {
    box-sizing: border-box;
    display: flex;
    flex-grow: 1;
    justify-content: space-around;
    padding: 0 10rpx;
}

.title-arrow {
    height: 19rpx;
    margin-left: 10rpx;
    transition: all .5s;
    width: 25rpx;
}

.arrTitle {
