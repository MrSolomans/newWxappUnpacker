<view class="ma-warp" hidden="{{!show}}">
    <view class="ma-mask">
        <view>
            <view bind:tap="goMemberGift" class="ma-main show" style="background: url({{alert_bg}}) no-repeat;background-size: 100% 100%;">
                <view class="ma-gift-title">
                    <view class="text1">您</view>
                    <view class="text2">的</view>
                    <view class="text3">{{month}}</view>
                    <view class="text4">月</view>
                    <view class="text5">会</view>
                    <view class="text6">员</view>
                    <view class="text7">礼</view>
                    <view class="text8">包</view>
                </view>
                <view class="ma-card-box">
                    <view class="ma-card-list">
                        <view class="ma-card-item" wx:if="{{giftList.length>1}}" wx:for="{{giftList}}" wx:key="index">
                            <view class="ma-card-gift-cont">
                                <view class="gift-info">
                                    <image class="gift-icon" src="https://hcz-static.pingan.com.cn/cps/hcz-miniprogram/images/memberAlert/alert_icon0{{filter.numberToIndex(index)}}.png"></image>
                                    <view class="gift-name">{{item.profitName}}</view>
                                </view>
                                <image class="gift-btn" src="{{alert_btn_s}}"></image>
                            </view>
                        </view>
                        <view class="ma-card-item" wx:if="{{giftList.length>0&&giftList.length<2}}" wx:for="{{giftList[0].couponDetail}}" wx:key="index">
                            <view class="ma-card-dis-cont" wx:if="{{filter.typeToBoolean( item.subType,['2-3','2-7','2-8','2-9','2-11','2-12','2-25','2-26','2-10','2-13','2-14','2-21','2-15','2-16','2-22','2-23','2-24'] )}}">
                                <view class="dis-info">
                                    <view class="dis-count" wx:if="{{filter.typeToBoolean( item.subType,['2-10','2-13','2-14','2-21'] )}}">{{item.discountNum}}<text class="dis-count-unit">折</text>
                                    </view>
                                    <view class="dis-count" wx:if="{{filter.typeToBoolean( item.subType,['2-3','2-7','2-8','2-9','2-11','2-12','2-25','2-26','2-15','2-16','2-22'] )}}">{{item.amountDeducted}}<text class="dis-count-unit">元</text>
                                    </view>
                                    <view class="dis-count" wx:if="{{filter.typeToBoolean( item.subType,['2-23','2-24'] )}}">{{item.fixedPayments}}<text class="dis-count-unit">元购</text>
                                    </view>
                                    <view class="dis-cont">
                                        <view class="dis-name">{{item.name}}</view>
                                        <view class="dis-des" wx:if="{{filter.typeToBoolean( item.subType,['2-3','2-7','2-8','2-9','2-11','2-12','2-25','2-26'] )&&item.fullPriceReduction}}">满{{item.fullPriceReduction}}元可用</view>
                                    </view>
                                </view>
                            </view>
                            <view class="ma-card-card-cont" wx:else>
                                <view class="card-info">
                                    <view class="card-icon-box">
                                        <image class="card-icon" src="{{alert_icon04}}"></image>
                                    </view>
                                    <view class="card-name">{{item.name}}</view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
                <view class="ma-main-bottom" style="background: url({{alert_bg_b}}) no-repeat;background-size: 100% 100%;">
                    <view class="ma-btn" style="background: url({{alert_btn_b}}) no-repeat;background-size: 100% 100%;"></view>
                </view>
            </view>
            <image bind:tap="closeAlert" class="ma-close" src="data:image/png;base64,"></image>
        </view>
    </view>
</view>
<wxs module="filter" src="memberAlert.wxs" />