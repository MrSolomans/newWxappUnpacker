.k-tag {
    box-sizing: border-box;
    color: #fff;
    display: inline-flex;
    font-family: PingFangSC-Regular;
    justify-content: center;
}

.k-tag,.k-tag-icon {
    align-items: center;
}

.k-tag-icon {
    display: flex;
    margin-left: var(--d0-distance);
    margin-right: 8rpx;
}

.k-tag-touch {
    min-width: 80rpx;
}

.k-tag-super-mini {
    font-size: 20rpx;
    height: 32rpx;
}

.k-tag-mini {
    font-size: 20rpx;
    height: 42rpx;
}

.k-tag-medium {
    font-size: 20rpx;
    height: 33rpx;
}

.k-tag-large {
    font-size: 24rpx;
    height: 60rpx;
    letter-spacing: 2rpx;
}

.k-tag-super-mini-square {
    border-radius: 2rpx;
    padding: 0 8rpx;
}

.k-tag-mini-square {
    border-radius: 2rpx;
    padding: 0 12rpx;
}

.k-tag-medium-square {
    border-radius: 4rpx;
    padding: 0 8rpx;
}

.k-tag-large-square {
    border-radius: 2rpx;
    padding: 0 18rpx;
}

.k-tag-super-mini-circle {
    border-radius: 16rpx;
    padding: 0 12rpx;
}

.k-tag-mini-circle {
    border-radius: 21rpx;
    padding: 0 16rpx;
}

.k-tag-medium-circle {
    border-radius: 25rpx;
    padding: 0 20rpx;
}

.k-tag-large-circle {
    border-radius: 30rpx;
    padding: 0 24rpx;
}

.k-tag-plain {
    background-color: #fff;
    border: 1rpx solid #000;
    color: #000;
}

.k-tag-plain-super-mini {
    font-size: 20rpx;
    height: 28rpx;
}

.k-tag-plain-mini {
    font-size: 20rpx;
    height: 38rpx;
}

.k-tag-plain-medium {
    font-size: 20rpx;
    height: 33rpx;
}

.k-tag-plain-large {
    font-size: 24rpx;
    height: 56rpx;
    letter-spacing: 2rpx;
}

.k-tag-disable {
    border-color: #000;
}

.k-tag-disable,.select {
    background-color: #000;
    color: #fff;
}

.content,.content-l {
    align-items: center;
    display: inline-flex;
    justify-content: center;
}

.content-l {
    flex-direction: row-reverse;
}

.tag-image-mini {
    height: 20rpx;
    width: 20rpx;
}

.tag-image-large {
    height: 24rpx;
    width: 24rpx;
}

.icon-class {
    margin-left: 4rpx;
}