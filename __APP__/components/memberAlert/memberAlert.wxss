.ma-warp {
    height: 100vh;
    overflow-x: hidden;
    position: fixed;
    width: 100vw;
    z-index: 10000;
}

.ma-mask {
    align-items: center;
    background: rgba(0,0,0,.6);
    bottom: 0;
    display: flex;
    height: 100%;
    justify-content: center;
    left: 0;
    overflow: hidden;
    position: fixed;
    right: 0;
    top: 0;
    width: 100%;
}

.ma-main {
    height: 644rpx;
    margin-bottom: 53rpx;
    padding: 156rpx 58rpx 0;
    position: relative;
    width: 484rpx;
}

.ma-gift-title {
    color: #fff;
    display: flex;
    font-size: 34rpx;
    font-weight: 700;
    height: 89rpx;
    justify-content: center;
    line-height: 42rpx;
    margin: 0 auto;
    text-shadow: 1rpx 1rpx 4rpx rgba(0,0,0,.5);
    width: 324rpx;
}

.text1 {
    transform: rotate(6deg);
}

.text2 {
    margin-top: 4rpx;
    transform: rotate(5deg);
}

.text3 {
    margin-top: 6rpx;
    transform: rotate(2deg);
}

.text4 {
    transform: rotate(1deg);
}

.text4,.text5 {
    margin-top: 8rpx;
}

.text5 {
    transform: rotate(0deg);
}

.text6 {
    margin-top: 6rpx;
    transform: rotate(-3deg);
}

.text7 {
    margin-top: 4rpx;
    transform: rotate(-5deg);
}

.text8 {
    transform: rotate(-6deg);
}

.ma-card-box {
    -webkit-overflow-scrolling: touch;
    height: 398rpx;
    overflow-y: scroll;
}

.ma-card-item {
    height: 126rpx;
    margin-bottom: 10rpx;
    width: 484rpx;
}

.ma-card-gift-cont {
    background: url(data:image/png;base64,)no-repeat;
    background-size: 100% 100%;
    height: 126rpx;
    justify-content: space-between;
    padding: 0 32rpx 0 25rpx;
}

.gift-info,.ma-card-gift-cont {
    align-items: center;
    display: flex;
}

.gift-info {
    justify-content: start;
}

.gift-icon {
    height: 76rpx;
    margin-right: 20rpx;
    width: 76rpx;
}

.gift-name {
    color: #222;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 34rpx;
    font-weight: 700;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 267rpx;
}

.gift-btn {
    height: 116rpx;
    width: 44rpx;
}

.ma-card-dis-cont {
    background: url(data:image/png;base64,)no-repeat;
    background-size: 100% 100%;
    height: 126rpx;
    justify-content: space-between;
}

.dis-info,.ma-card-dis-cont {
    align-items: center;
    display: flex;
}

.dis-info {
    justify-content: start;
}

.dis-count {
    font-family: PingFangSC-Semibold,PingFang SC;
    font-size: 58rpx;
    margin-right: 37rpx;
    text-align: center;
    width: 152rpx;
}

.dis-count,.dis-count-unit {
    color: #ff6300;
    font-weight: 700;
}

.dis-count-unit {
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 24rpx;
}

.dis-name {
    color: #222;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 34rpx;
    font-weight: 700;
    line-height: 34rpx;
}

.dis-des,.dis-name {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 285rpx;
}

.dis-des {
    color: #a5a5a5;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 24rpx;
    line-height: 24rpx;
    margin-top: 14rpx;
}

.ma-card-card-cont {
    background: url(data:image/png;base64,)no-repeat;
    background-size: 100% 100%;
    height: 126rpx;
    justify-content: space-between;
}

.card-info,.ma-card-card-cont {
    align-items: center;
    display: flex;
}

.card-info {
    justify-content: start;
}

.card-icon-box {
    margin-right: 37rpx;
    text-align: center;
    width: 152rpx;
}

.card-icon {
    height: 56rpx;
    width: 103rpx;
}

.card-name {
    color: #222;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 34rpx;
    font-weight: 700;
    line-height: 34rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 285rpx;
}

.ma-main-bottom {
    bottom: 5rpx;
    height: 132rpx;
    left: 24rpx;
    padding-top: 108rpx;
    position: absolute;
    width: 554rpx;
}

.ma-btn {
    height: 118rpx;
    margin: 0 auto;
    width: 438rpx;
}

.ma-close {
    display: block;
    height: 66rpx;
    margin: 0 auto;
    width: 66rpx;
}

.show {
    animation: popup-center-fadein .4s;
}

@-webkit-keyframes popup-center-fadein {
    0% {
        opacity: 0;
        transform: scale(.8);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes popup-center-fadein {
    0% {
        opacity: 0;
        transform: scale(.8);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}