.detect-border::before {
    background: rgba(255,0,0,.1);
    border: 1px dashed red;
    content: "";
    height: 100%;
    padding: 10rpx;
    position: absolute;
    width: 100%;
    z-index: 999;
}

@font-face {
    font-family:DIN;src:url(../image/DINAlternate.ttf-do-not-use-local-path-./miniprogram_npm/@kun/kun-mp-ui-5/tab/index.wxss&1&4589);
}

.nav-content {
    background: #fff;
    position: relative;
    width: 100%;
}

.nav-scroll {
    position: relative;
    white-space: nowrap;
}

.nav-scroll-item {
    display: inline-block;
    vertical-align: top;
}

.nav-scrol-btn {
    color: var(--c1-color);
    font-size: var(--t5-font);
    height: 88rpx;
    letter-spacing: 0;
    line-height: 88rpx;
    overflow: hidden;
    text-align: center;
    text-overflow: ellipsis;
    transition: all .6s;
    white-space: nowrap;
}

.badge-class {
    position: absolute;
    right: 20rpx;
    top: 20rpx;
}

.nav-scroll::after,.nav-scroll::before {
    clear: both;
    content: "";
    display: table;
}

.nav-scroll-item>.nav-scrol-btn.tab-active {
    color: var(--c0-color);
    font-family: PingFangSC-Medium;
    font-weight: 500;
}

.nav-scroll-line {
    display: flex;
    justify-content: center;
    left: 0;
    position: absolute;
    top: 70rpx;
    transition: left .5s;
}

.line {
    background: var(--c7-color);
    border-radius: 3rpx;
    height: 6rpx;
    width: 32rpx;
}

.mask {
    background-image: linear-gradient(90deg,hsla(0,0%,100%,0),#fff);
    height: 88rpx;
    position: absolute;
    right: 0;
    top: 0;
    width: 88rpx;
}

.newLine {
    background: var(--c7-color);
    border-radius: 3rpx;
    height: 6rpx;
    top: 70rpx;
    transition: left .5s;
    width: 32rpx;
}

.moreDetail,.newLine {
    display: flex;
    justify-content: center;
    position: absolute;
}

.moreDetail {
    align-items: center;
    background: #fff;
    height: 88rpx;
    right: 0;
    top: 0;
    width: 88rpx;
}

.lineCenter {
    display: flex;
    justify-content: center;
}

.nav-tag-scroll {
    box-sizing: border-box;
    position: relative;
    white-space: nowrap;
}

.nav-tag-box {
    display: flex;
    margin-left: 30rpx;
}

.nav-scrol-tag {
    background: var(--c5-color);
    border-radius: 28rpx;
    color: var(--c1-color);
    font-size: var(--t6-font);
    height: 36rpx;
    letter-spacing: 0;
    line-height: 36rpx;
    margin-right: 20rpx;
    min-width: 60rpx;
    padding: 10rpx 20rpx;
    text-align: center;
}

.nav-tag-box>.nav-scrol-tag.tag-active {
    background: var(--c8-color);
    color: var(--c7-color);
    font-family: PingFangSC-Medium;
    font-weight: 500;
}

.nav-scroll-term {
    display: inline-block;
}

.nav-scrol-term-msg,.nav-scroll-term {
    align-items: center;
}

.nav-scrol-term-msg {
    color: var(--c1-color);
    display: flex;
    font-size: var(--t5-font);
    height: 80rpx;
    justify-content: center;
    letter-spacing: 0;
    line-height: 80rpx;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
}

.nav-scrol-term-msg,.show-Clamp {
    overflow: hidden;
    text-overflow: ellipsis;
}

.show-Clamp {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    display: -webkit-box;
}

.nav-scroll-term>.nav-scrol-term-msg.term-active {
    color: var(--c7-color);
}

.nav-scrol-term-msg>.arrow {
    border: 8rpx solid transparent;
    border-top: 10rpx solid #ccc;
    margin-top: 10rpx;
}

.nav-scrol-term-msg.term-active>.arrow,.nav-scrol-term-msg.term-active>.arrow-box>.arrow-active-down {
    border-top: 10rpx solid var(--c7-color);
}

.nav-scrol-term-msg>.arrow-box {
    margin-left: 6rpx;
    margin-top: -8rpx;
}

.nav-scrol-term-msg>.arrow-box>.arrow-top {
    border: 8rpx solid transparent;
    border-bottom: 10rpx solid #ccc;
    margin-bottom: 4rpx;
}

.nav-scrol-term-msg>.arrow-box>.arrow-bottom {
    border-color: #ccc transparent transparent;
    border-style: solid;
    border-width: 10rpx 8rpx 8rxp;
}

.nav-scrol-term-msg.term-active>.arrow-box>.arrow-active-up {
    border-bottom: 10rpx solid #ff8727;
}

.icon_edit,.icon_showDetail {
    height: 36rpx;
    width: 36rpx;
}

.left-border {
    background-image: linear-gradient(90deg,hsla(0,0%,86%,0) 22%,#d8d8d8);
    height: 88rpx;
    left: -16rpx;
    position: absolute;
    width: 16rpx;
}

.screenArrow2 {
    height: 24rpx;
    margin-left: 4rpx;
    width: 24rpx;
}

.nav-section {
    height: 100rpx;
    justify-content: center;
    position: relative;
    white-space: nowrap;
    width: 100vw;
}

.nav-inside,.nav-section {
    align-items: center;
    display: flex;
}

.nav-inside {
    background: var(--c5-color);
    border-radius: var(--r2-radius);
    color: var(--c1-color);
    font-size: var(--t6-font);
    height: 60rpx;
    text-align: center;
    width: 686rpx;
}

.nav-section-btn {
    border-radius: 36rpx;
    height: 60rpx;
    line-height: 60rpx;
}

.section-tag-active {
    background: var(--c7-color);
    color: var(--c6-color);
}