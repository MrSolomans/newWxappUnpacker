<view class="pa w100 h100 jfvideo">
    <live-pusher autopush aspect="{{jfVideoRoom.pusher.videoAspect}}" audioVolumeType="{{jfVideoRoom.pusher.audioVolumeType}}" beauty="{{jfVideoRoom.pusher.beauty}}" binderror="onPushError" bindnetstatus="onNetStatus" bindstatechange="onPush" class="pa {{jfVideoRoom.pusher.classes}}" devicePosition="{{jfVideoRoom.pusher.devicePosition}}" enableAns="{{true}}" enableCamera="{{jfVideoRoom.pusher.enableCamera}}" enableMic="{{jfVideoRoom.pusher.enableMic&&!jfVideoRoom.allMuted&&!jfVideoRoom.pusher.hiding}}" id="{{jfVideoRoom.pusher.id}}" maxBitrate="{{jfVideoRoom.pusher.maxBitrate}}" minBitrate="{{jfVideoRoom.pusher.minBitrate}}" mode="{{jfVideoRoom.pusher.mode}}" orientation="{{jfVideoRoom.pusher.videoOrientation}}" style="z-index:{{jfVideoRoom.pusher.zindex}};" url="{{jfVideoRoom.pusher.url}}" videoHeight="{{jfVideoRoom.pusher.videoHeight}}" videoWidth="{{jfVideoRoom.pusher.videoWidth}}" waitingImage="{{jfVideoRoom.pusher.waitingImage}}" waitingImageHash="{{jfVideoRoom.pusher.waitingImageHash}}" whiteness="{{jfVideoRoom.pusher.whiteness}}" wx:if="{{jfVideoRoom.pusher.url}}">
        <cover-view class="username" wx:if="{{jfVideoRoom.pusher.classes.indexOf('bigView')<0}}">【{{jfVideoRoom.pusher.data.nickname||'我'}}】</cover-view>
        <cover-view bindtap="toggleWindow" class="pa w100 h100" data-id="jfpusher" wx:if="{{jfVideoRoom.pusher.classes[0]!='bigView'}}"></cover-view>
        <cover-view wx:if="{{!jfVideoRoom.players||jfVideoRoom.players.length==0}}">
            <live-player mode="RTC"></live-player>
        </cover-view>
    </live-pusher>
    <view class="pa {{playerLandscapeType?'smallView'+index+'-landscape':player.classes}}" style="{{!playerShow&&'top:100vh; right:100vw;'}}" wx:for="{{jfVideoRoom.players}}" wx:for-item="player" wx:key="id">
        <live-player autoplay bindaudiovolumenotify="onPlayAudioVolumeNotify" bindnetstatus="onNetStatus" bindstatechange="onPlay" class="pa w100 h100" id="{{player.id}}" maxCache="{{player.maxCache}}" minCache="{{player.minCache}}" mode="RTC" muteVideo="{{player.muteVideo}}" muted="{{jfVideoRoom.allMuted||jfVideoRoom.remoteMuted||player.muteAudio}}" objectFit="{{player.objectFit}}" orientation="{{player.orientation}}" soundMode="{{soundMode}}" src="{{player.url}}" style="z-index:{{player.zindex}};" wx:if="{{player.url}}"></live-player>
        <image class="headphoto" src="{{userIcon}}" style="z-index:{{player.zindex}};" wx:if="{{!player.hasVideo}}"></image>
        <cover-view class="username" wx:if="{{player.classes.indexOf('bigView')<0}}">【{{player.data.nickname||player.uid}}】</cover-view>
        <cover-image class="mic" src="{{mic}}" wx:if="{{player.isSpeaking}}"></cover-image>
        <cover-view bindtap="toggleWindow" class="pa w100 h100" data-id="{{player.id}}" wx:if="{{player.classes[0]!='bigView'&&orientation==1}}"></cover-view>
    </view>
</view>
