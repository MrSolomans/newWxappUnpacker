.cirqueBox {
    animation: loading 1s linear infinite;
    border-radius: 50%;
    height: 36rpx;
    margin-right: 14rpx;
    mask: radial-gradient(transparent 16rpx,#000 0);
    position: relative;
    width: 36rpx;
}

.cirqueBox-small {
    height: 27rpx;
    mask: radial-gradient(transparent 11rpx,#000 0);
    width: 27rpx;
}

.cirqueRed {
    background-color: var(--c7-color);
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 1;
}

.cirqueRed-warning {
    background: var(--c10-color);
}

.cirqueRed-outline {
    background-color: #fff;
}

.cirqueRed-black {
    background: 0 0;
}

.cirqueRed-gray {
    background: var(--c3-color);
}

.cirqueGrey {
    background-color: #fff;
    clip-path: polygon(18rpx 18rpx,18rpx -100rpx,-100rpx 18rpx,18rpx 200rpx,200rpx 18rpx,100rpx 18rpx);
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 2;
}

.cirqueGrey-small {
    clip-path: polygon(10rpx 10rpx,10rpx -100rpx,-100rpx 10rpx,10rpx 200rpx,200rpx 10rpx,100rpx 10rpx);
}

.cirqueGrey-outline {
    background: var(--c7-color);
}

.cirqueGrey-black {
    background: #fff;
}

.leftIcon {
    margin-right: 15rpx;
}

.loading {
    border-radius: 50%;
    height: 100rpx;
    overflow: hidden;
    padding: 10rpx;
    position: relative;
    width: 100rpx;
}

.loading .loading-content {
    border-radius: 50%;
    height: 100%;
    position: relative;
    width: 100%;
    z-index: 888;
}

.loading .loading-border {
    border: 10px solid #f0f1f6;
    border-radius: 50%;
    z-index: 666;
}

.loading .loading-border,.loading .loading-mask {
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
}

.loading .loading-mask {
    animation: rotateAnimate 2s linear infinite;
    z-index: 777;
}

.loading .loading-mask::before {
    background-color: #6c8de5;
    border-radius: 50%;
    bottom: 0;
    content: "";
    display: block;
    height: 10px;
    left: calc(45%);
    position: absolute;
    width: 10px;
}

.loading .loading-mask::after {
    background-image: linear-gradient(45deg,#6c8de5,#f0f1f6);
    bottom: 0;
    content: "";
    display: block;
    height: 60%;
    left: 50%;
    position: absolute;
    width: 50%;
}

@-webkit-keyframes rotateAnimate {
    from {
        transform: rotate(0);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes rotateAnimate {
    from {
        transform: rotate(0);
    }

    to {
        transform: rotate(1turn);
    }
}