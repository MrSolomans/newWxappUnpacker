.home_page {
    background: #fff;
    border-radius: 16rpx;
    box-sizing: border-box;
    height: 150rpx;
    margin: 30rpx auto;
    overflow: hidden;
    width: 690rpx;
}

.cr-swiper {
    height: 100%;
    width: 100%;
}

.home_page .cr-swiper_item {
    box-sizing: border-box;
    height: 150rpx;
    padding: 32rpx 30rpx 36rpx;
    width: 690rpx;
}

.wrap_renewal_icon {
    background: url(data:image/png;base64,)no-repeat 0 0/128rpx 42rpx;
    border-radius: 21rpx;
    height: 42rpx;
    width: 128rpx;
}

.wrap_renewal_text {
    font-size: 24rpx;
    line-height: 28rpx;
    margin-top: 16rpx;
}

.wrap_renewal_btn {
    background: url(data:image/png;base64,)no-repeat 0 0/140rpx 56rpx;
    bottom: 30rpx;
    height: 56rpx;
    position: absolute;
    right: 30rpx;
    width: 140rpx;
}

.right_page {
    margin: 0 auto;
    width: 630rpx;
}

.wrap_msg {
    align-items: center;
    background: #f6f7f8;
    border-radius: 16rpx;
    display: flex;
    height: 88rpx;
    justify-content: center;
    overflow: hidden;
    width: 630rpx;
}

.wrap_msg_icon {
    background: url(data:image/png;base64,)no-repeat 0 0/32rpx 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
    width: 32rpx;
}

.wrap_msg_text {
    color: #3d3d3d;
    font-size: 28rpx;
    line-height: 28rpx;
    margin-right: 35rpx;
}

.wrap_msg_btn {
    background: url(data:image/png;base64,)no-repeat 0 0/120rpx 48rpx;
    height: 48rpx;
    width: 120rpx;
}

.wrap_renewal .cr-swiper_item {
    box-sizing: border-box;
    padding: 20rpx;
}

.wrap_renewal .wrap_renewal_btn {
    bottom: 20rpx;
    right: 20rpx;
}

.wrap_renewal .wrap_renewal_text {
    margin-top: 20rpx;
}

.warning_color {
    color: #ff8727;
}

.wrap_renewal_detail {
    background: #f6f7f8;
    border-radius: 16rpx;
    height: 128rpx;
    overflow: hidden;
    width: 100%;
}

.wrap_renewal_triangle {
    background: #f6f7f8;
    height: 24rpx;
    margin-bottom: -12rpx;
    margin-left: 178rpx;
    transform: translateX(-50%) rotate(45deg);
    width: 24rpx;
}