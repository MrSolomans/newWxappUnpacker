}

.k-btn-gray:before,.k-btn-outline:before {
    border: 1px solid var(--c7-color);
    border-radius: var(--r0-radius);
    box-sizing: border-box;
    content: "";
    height: 200%;
    position: absolute;
    transform: scale(.5,.5);
