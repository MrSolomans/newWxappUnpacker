.aw-container {
    height: 100vh;
    overflow-x: hidden;
    width: 100vw;
    z-index: 9999;
}

.aw-container,.aw-mask {
    left: 0;
    position: fixed;
    top: 0;
}

.aw-mask {
    align-items: center;
    background-color: rgba(0,0,0,.6);
    display: flex;
    height: 100%;
    justify-content: center;
    overflow: hidden;
    width: 100%;
    z-index: 10000;
}

.aw-window {
    background-color: #fff;
    border-radius: 16rpx;
    height: 721rpx;
    width: 600rpx;
}

@-webkit-keyframes windowShow {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes windowShow {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

.aw-window .title {
    color: #222;
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    font-weight: 500;
    line-height: 36rpx;
    padding: 56rpx 0 32rpx;
    text-align: center;
}

.aw-window .content-box {
    background-color: #f6f7f8;
    border-radius: 16px;
    box-sizing: border-box;
    height: 468rpx;
    margin: 0 auto 28rpx;
    overflow-y: auto;
    padding: 24rpx;
    width: 552rpx;
}

.aw-window .content-box .content {
    color: #666;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    font-weight: 400;
}

.aw-window .content-box .content text {
    color: #ff6300;
}

.aw-window .rules {
    color: #657aab;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 28rpx;
    padding: 28rpx 0 32rpx 24rpx;
}

.aw-window .content-other {
    background-color: #fff;
    font-size: 30rpx;
    height: 494rpx;
    padding: 0 30rpx;
}

.aw-window .content-other .read {
    font-weight: 700;
    margin: 20rpx 0 15rpx;
}

.aw-window .content-other .info {
    align-items: center;
    color: #666;
    display: flex;
    font-size: 28rpx;
    font-weight: 400;
    height: 48rpx;
    line-height: 36rpx;
}

.aw-window .content-other .checkbox {
    height: 48rpx;
    width: 48rpx;
}

.aw-window .content-other .treaty {
    color: #90a0c0;
}

.aw-window .btn-box {
    border-top: 1rpx solid #e5e5e5;
    box-sizing: border-box;
    display: flex;
    height: 100rpx;
    width: 600rpx;
}

.aw-window .btn-box .agree-button,.aw-window .btn-box .navigator-exit,.aw-window .btn-box>view {
    align-items: center;
    background-color: initial;
    border-radius: 0;
    box-sizing: border-box;
    display: flex;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    font-weight: 500;
    height: 98rpx;
    justify-content: center;
    letter-spacing: 0;
    width: 300rpx;
}

.aw-window .btn-box .no-agree {
    border-right: 1rpx solid #e5e5e5;
    color: #666;
}

.aw-window .btn-box .agree {
    color: #ff6300;
}