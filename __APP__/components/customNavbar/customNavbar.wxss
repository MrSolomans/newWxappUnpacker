.customNavBarWrap {
    background-color: #2f2f2f;
}

.customHomeNavBarWrap,.customNavBarWrap {
    left: 0;
    position: fixed;
    top: 0;
    width: 100vw;
    z-index: 9999;
}

.cm-con {
    height: 43px;
    left: 0;
    top: 0;
    width: 100vw;
    z-index: 9999;
}

.customNavBarBox {
    height: 43px;
    position: relative;
}

.customNavBarBox,.leftBtnArea {
    align-items: center;
    display: flex;
}

.leftBtnArea {
    background: hsla(0,0%,100%,.6);
    border: 1px solid transparent;
    border-radius: 60px;
    box-sizing: border-box;
    height: 64px;
    left: 7px;
    position: fixed;
    top: 26px;
    transform: scale(.5);
    transform-origin: left top;
    transition: top .2s;
    width: 176px;
}

.leftBtnArea.black {
    border-color: rgba(0,0,0,.08);
}

.leftBtnArea.white {
    border-color: hsla(0,0%,100%,.2);
}

.leftBtnArea.needBlackBg {
    background: rgba(0,0,0,.15);
}

.leftBtnArea.needBlackBg.android {
    background: rgba(0,0,0,.18);
}

.leftBtnArea>.view {
    box-sizing: border-box;
    height: 37px;
    position: relative;
    width: 86px;
}

.leftBtnArea>.view:last-child {
    border-left: 1px solid transparent;
}

.leftBtnArea.singleBack,.leftBtnArea.singleHome {
    width: 64px;
}

.leftBtnArea.black>.view:first-child {
    border-right: 1px solid rgba(0,0,0,.08);
}

.leftBtnArea.white>.view:first-child {
    border-right: 1px solid hsla(0,0%,100%,.2);
}

.leftBtnArea.singleBack>.view:first-child,.leftBtnArea.singleHome>.view:first-child {
    border-right: none;
}

.leftBtnArea .image {
    height: 43px;
    left: 50%;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 43px;
}

.leftBtnArea .iconImg {
    background-size: 100%auto;
    height: 43px;
    width: 43px;
}

.customTitle {
    color: #fff;
    font-family: PingFangSC-Medium,PingFangSC;
    font-weight: 700;
    height: 100%;
    left: 104px;
    letter-spacing: 0;
    line-height: 43px;
    overflow: hidden;
    position: absolute;
    right: 104px;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
}

