.search-container,.swiper_container {
    align-items: center;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.swiper_container {
    flex-grow: 1;
    height: 31px;
}

.swiper_item {
    align-items: center;
    color: #ccc;
    font-size: 13px;
    height: 31px;
    justify-content: flex-start;
    line-height: 31px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.region-picker,.swiper_item {
    display: flex;
    flex-direction: row;
}

.region-picker {
    padding-left: 5px;
}

.region-picker-item {
    color: #666;
    font-family: PingFangSC-Medium;
    font-size: 14px;
    font-weight: 500;
    height: 20px;
    line-height: 20px;
    max-width: 56px;
    overflow: hidden;
    text-align: left;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.region-arrow {
    height: 18px;
    width: 18px;
}

.region-weather {
    color: #666;
    font-family: PingFangSC-Regular;
    font-size: 9px;
    font-weight: 400;
    height: 13px;
    letter-spacing: 0;
    line-height: 13px;
    width: 54px;
}

.cm-search {
    border: 0;
    color: var(--c3-color);
    flex-grow: 1;
    font-size: var(--t5-font);
    padding-right: 10px;
}

.cancel-search {
    color: var(--c0-color);
    font-family: PingFangSC-Regular;
    font-size: var(--t6-font);
    font-weight: 400;
    height: 32px;
    line-height: 32px;
    margin-left: 10px;
    width: 35px;
}

.in-region {
    border-right: 2px solid #ccc;
    padding: 0 5px;
}

.title-screening {
    background-color: #fff;
    border-bottom-left-radius: 24rpx;
    border-bottom-right-radius: 24rpx;
    box-sizing: border-box;
    max-height: 336rpx;
    min-height: 216rpx;
    position: fixed;
    transition: all .3s;
    width: 100%;
    z-index: 999;
}

.title-window-mask {
    background: rgba(0,0,0,.5);
    height: 100%;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 998;
}

.title-screening-box {
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-around;
    max-height: 216rpx;
    padding: 20rpx 20rpx 0;
}

.title-screening-item {
    background: #f6f7f8;
    border-radius: var(--r2-radius);
    color: var(--c0-color);
    font-size: var(--t7-font);
    height: 60rpx;
    line-height: 60rpx;
    margin: 14rpx auto;
    text-align: center;
    width: 213rpx;
}

.screening-selected {
    background: var(--c8-color);
