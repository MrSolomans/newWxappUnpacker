.k-badge {
    line-height: 1;
    position: relative;
}

.k-badge-content {
    background: #ff3526;
    border: 2rpx solid #fff;
    box-sizing: border-box;
    color: #fff;
    display: inline-block;
    font-family: PingFangSC-Medium;
    font-size: 20rpx;
    font-weight: 500;
    letter-spacing: 0;
    max-width: 650rpx;
    overflow: hidden;
    position: absolute;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
    z-index: 10;
}

.k-badge-content-horn {
    border-radius: 16rpx 16rpx 16rpx 4rpx;
    min-height: 32rpx;
    min-width: 40rpx;
    padding: 6rpx 10rpx;
    top: -20rpx;
}

.k-badge-content-circle {
    border-radius: 16rpx;
    letter-spacing: 0;
    min-height: 32rpx;
    min-width: 32rpx;
    padding: 5rpx 9rpx;
    text-align: center;
    top: -10rpx;
}

.k-badge-dot {
    background: #ff3526;
    border: 2rpx solid #fff;
    border-radius: 50%;
    height: 20rpx;
    position: absolute;
    right: 0rpx;
    top: -4rpx;
    transform: translateX(50%);
    width: 20rpx;
}