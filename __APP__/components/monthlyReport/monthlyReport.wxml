<view bind:tap="checkUserSubscription" class="monthly-con" wx:if="{{showMonthly}}">
    <view>
        <view class="monthly-font">您的{{month}}月保险月报已生成</view>
    </view>
    <view class="monthly-right">
        <view class="monthly-font">请查收</view>
        <image class="monthly-img monthly-img_right" src="data:image/png;base64,"></image>
        <view catch:tap="close" wx:if="{{showClose}}">
            <image class="monthly-img monthly-img_close" src="data:image/png;base64,"></image>
        </view>
    </view>
</view>
