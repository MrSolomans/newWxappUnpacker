.go-app {
    background: transparent;
    bottom: 0;
    left: 0;
    opacity: 0;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 10;
}

.customSearch {
    align-items: center;
    background-color: #fff;
    border-radius: 25px;
    box-sizing: border-box;
    color: #ccc;
    display: flex;
    font-size: 28rpx;
}

.customSearch .icon-small {
    height: 32rpx;
    left: 12rpx;
    margin-right: 8rpx;
    margin-top: -16rpx;
    position: absolute;
    top: 50%;
    width: 32rpx;
}

.container {
    align-items: center;
    background: #ededed;
    display: flex;
    flex-direction: row;
    height: 80rpx;
    justify-content: center;
    width: 100%;
}

.search-container {
    background: #fff;
    border-radius: 5rpx;
    height: 60rpx;
    width: 690rpx;
}

.search-container,.swiper_container {
    align-items: center;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.swiper_container {
    height: 62rpx;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 1;
}

.swiper_item {
    align-items: center;
    color: #999;
    display: flex;
    flex-direction: row;
    font-size: 14px;
    height: 62rpx;
    justify-content: flex-start;
    line-height: 62rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.customCityBox {
    border-right: 2rpx solid #e5e5e5;
    color: #666;
    height: 28rpx;
    line-height: 28rpx;
    padding-right: 12rpx;
    width: 88rpx;
}

.customCityName {
    display: inline-block;
    overflow: hidden;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 64rpx;
}

.customCityIcon {
    height: 24rpx;
    width: 24rpx;
}

.customSearchContent {
    position: relative;
}

.region-box {
    align-items: center;
    display: flex;
    flex-wrap: nowrap;
    justify-content: center;
    margin-left: 40rpx;
}

.region-name {
    color: #fff;
    font-size: 28rpx;
    margin-right: 5rpx;
    max-width: 200rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.region-picker {
    align-items: center;
    display: flex;
}

.plate-num {
    color: #fff;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 28rpx;
    font-weight: 500;
}

.car-logo {
    height: 32rpx;
    margin-right: 10rpx;
    width: 32rpx;
}

.car-info {
    align-items: center;
    display: flex;
}

.add-car-icon {
    height: 36rpx;
    margin-right: 10rpx;
    width: 36rpx;
}