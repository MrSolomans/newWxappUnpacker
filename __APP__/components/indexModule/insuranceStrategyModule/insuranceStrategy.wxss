.insurance_strategy {
    background: #fff;
    border-radius: 20rpx;
    margin: 20rpx auto 0;
    padding-bottom: 30rpx;
    padding-top: 30rpx;
    width: 690rpx;
}

.wrap_top {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 0rpx 30rpx 30rpx;
}

.wrap_title {
    color: #222;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-size: 32rpx;
    font-weight: 600;
    line-height: 32rpx;
}

.wrap_more {
    color: #50575d;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 28rpx;
    margin-top: 2rpx;
}

.wrap_more_img {
    height: 20rpx;
    margin-left: 8rpx;
    width: 12rpx;
}

.wrap_content {
    margin-left: 30rpx;
    overflow: hidden;
    white-space: nowrap;
}

.insurance_strategy_content {
    display: inline-block;
    height: 174rpx;
    margin-right: 10rpx;
    overflow: hidden;
    position: relative;
    width: 290rpx;
}

.insurance_strategy_BgColor,.insurance_strategy_img {
    border-radius: 16rpx;
    height: 100%;
    width: 100%;
}

.insurance_strategy_BgColor {
    background: #000;
    left: 0rpx;
    opacity: .5;
    position: absolute;
    top: 0rpx;
}

.insurance_strategy_box {
    height: 100%;
    left: 0rpx;
    position: absolute;
    top: 0rpx;
    width: 100%;
}

.insurance_strategy_title {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #fff;
    display: -webkit-box;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 30rpx;
    font-weight: 500;
    margin: 14rpx 20rpx 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: pre-line;
    word-break: break-all;
}

.insurance_strategy_numBox {
    bottom: 20rpx;
    height: 32rpx;
    left: 20rpx;
    position: absolute;
}

.insurance_strategy_numBox_img {
    float: left;
    margin-right: 12rpx;
}

.insurance_strategy_numBox_img .insurance_strategy_numBox_userImg:nth-child(2),.insurance_strategy_numBox_img .insurance_strategy_numBox_userImg:nth-child(3) {
    margin-left: -10rpx;
}

.insurance_strategy_numBox_userImg {
    border: 1rpx solid #fff;
    border-radius: 50%;
    height: 32rpx;
    width: 32rpx;
}

.insurance_strategy_numBox_text {
    color: #fff;
    float: left;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 22rpx;
    font-weight: 400;
}